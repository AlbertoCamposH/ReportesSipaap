	<subDataset name="datos" uuid="aaf8c48a-be78-44b6-8223-cb80e6081a77">
		<parameter name="ur" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="periodo" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="folio" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT id_duep_materiales_almacen, CONCAT(be.nombre,' ',be.primer_apellido, ' ', be.segundo_apellido) AS beneficiario,
CONCAT(contrato,'/',orde_compra) AS contrato, ma.fecha_comprobante, centro_registro, ''total_comprometido, ''comprometido_no_devengado
FROM operation.materiales_almacen ma
INNER JOIN operation.detalle_materiales_almacen dma
ON ma.id_materiales_almacen = dma.id_materiales_almacen
INNER JOIN entity.beneficiario be
ON ma.id_beneficiario = be.id_beneficiario
WHERE ma.ur = $P{ur} AND ma.id_periodo =$P{periodo} AND ma.id_materiales_almacen = $P{folio}]]>
		</queryString>
		<field name="id_duep_materiales_almacen" class="java.lang.String"/>
		<field name="beneficiario" class="java.lang.String"/>
		<field name="contrato" class="java.lang.String"/>
		<field name="fecha_comprobante" class="java.sql.Date"/>
		<field name="centro_registro" class="java.lang.String"/>
		<field name="total_comprometido" class="java.lang.String"/>
		<field name="comprometido_no_devengado" class="java.lang.String"/>
	</subDataset>
	<parameter name="ur" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="periodo" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="folio" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select a.id_unidad_responsable, to_char(current_date,'dd/mm/yyyy'), to_char(current_date,'yyyy') b, u.nombre_usuario
FROM entity.usuario u
INNER JOIN entity.mascara_articulo a ON u.id_usuario = a.ultimo_usuario_modificador
where a.id_unidad_responsable =$P{ur}
LIMIT 1]]>
	</queryString>
	<field name="id_unidad_responsable" class="java.lang.Long"/>
	<field name="to_char" class="java.lang.String"/>
	<field name="b" class="java.lang.String"/>
	<field name="nombre_usuario" class="java.lang.String"/>
