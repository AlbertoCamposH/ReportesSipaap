		<queryString>
			<![CDATA[SELECT c.id_periodo, c.tipo_costeo, c.fecha_elaboracion, u.nombre_usuario FROM catalog.tipo_costeo AS c
INNER JOIN entity.usuario AS u ON (u.id_unidad_responsable = c.ur AND u.id_usuario = c.usuario_alta)
WHERE c.ur = $P{ur} AND c.id_periodo = $P{periodo} AND c.id_tipo_costeo = $P{tipo_costeo}]]>
		</queryString>
		<field name="id_periodo" class="java.lang.Long"/>
		<field name="tipo_costeo" class="java.lang.String"/>
		<field name="fecha_elaboracion" class="java.sql.Timestamp"/>
