	<subDataset name="tabla" uuid="f920a002-90bc-4f6a-90c8-a8d3299c300e">
		<parameter name="folio_operacion_instancia" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="id_unidad_responsable" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select beneficiario.fecha_registro, beneficiario.id_beneficiario, beneficiario.razon_social,
concat_ws(' ', beneficiario.nombre, beneficiario.primer_apellido) nombre,
beneficiario.rfc, beneficiario.curp,
estatus.estatus
from entity.beneficiario_aux beneficiario
full outer join catalog.estatus estatus on estatus.id = beneficiario.id_estatus_beneficiario
where beneficiario.id_unidad_responsable =$P{id_unidad_responsable} and folio_operacion_instancia=$P{folio_operacion_instancia}]]>
		</queryString>
		<field name="fecha_registro" class="java.sql.Timestamp"/>
		<field name="id_beneficiario" class="java.lang.Long"/>
		<field name="razon_social" class="java.lang.String"/>
		<field name="nombre" class="java.lang.String"/>
		<field name="rfc" class="java.lang.String"/>
		<field name="curp" class="java.lang.String"/>
		<field name="estatus" class="java.lang.String"/>
	</subDataset>
	<parameter name="id_unidad_responsable" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="id_periodo" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="folio_operacion_instancia" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select a.id_unidad_responsable, to_char(current_date,'dd/mm/yyyy'), to_char(current_date,'yyyy') b, usuario.nombre_usuario from entity.beneficiario a
left join entity.usuario usuario on  usuario.id_usuario = a.id_ultimo_usuario_modificador
where a.id_unidad_responsable = $P{id_unidad_responsable} and a.id_periodo = $P{id_periodo} limit 1]]>
	</queryString>
