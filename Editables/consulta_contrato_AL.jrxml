	<subDataset name="datos" uuid="0b660a16-0111-44f0-83f8-9500f0440a2c">
		<queryString language="SQL">
			<![CDATA[SELECT (art.nivel) AS "Clave de nivel", (ma.descripcion) AS "Descripción de nivel", (art.clave_articulo) AS "Clave", (art.descripcion_articulo) AS "Descripción", (um.descripcion) AS "Unidad de medida",CASE WHEN art.afectable = false THEN 'NO AFECTABLE' WHEN art.afectable = true THEN 'AFECTABLE' ELSE 'NA' END AS "Afectable", (art.clasificacion_objeto_gasto) AS "Clasificador por Objeto de Gasto", (art.descripcion_objeto_gasto) AS "Descripción COG", (art.estatus) AS "Estatus",CASE WHEN art.autorizado = false THEN 'NO AUTORIZADO' WHEN art.afectable = true THEN 'AUTORIZADO' ELSE 'NA' END AS "Autorizado"
,CONCAT (usr.nombre,' ',usr.primer_apellido) AS "Último usuario modificador"
FROM catalog.articulos_aux art

INNER JOIN entity.mascara_articulo ma ON ma.nivel = art.nivel
INNER JOIN catalog.unidad_medida um ON um.id_unidad_medida = art.unidad_medida
FULL OUTER JOIN entity.usuario usr ON usr.id_usuario = art.ultimo_usuario_modificador
WHERE unidad_medida = um.id_unidad_medida
AND art.periodo =2019 AND um.id_periodo = art.periodo AND ma.id_periodo = art.periodo
AND art.ur = 1 AND um.id_unidad_responsable = art.ur AND ma.id_unidad_responsable = art.ur
AND art.id_instance =42
ORDER BY art.nivel]]>
		</queryString>
		<field name="Clave de nivel" class="java.lang.Long"/>
		<field name="Descripción de nivel" class="java.lang.String"/>
		<field name="Clave" class="java.lang.String"/>
		<field name="Descripción" class="java.lang.String"/>
		<field name="Unidad de medida" class="java.lang.String"/>
		<field name="Afectable" class="java.lang.String"/>
		<field name="Clasificador por Objeto de Gasto" class="java.lang.Long"/>
		<field name="Descripción COG" class="java.lang.String"/>
		<field name="Estatus" class="java.lang.String"/>
		<field name="Autorizado" class="java.lang.String"/>
		<field name="Último usuario modificador" class="java.lang.String"/>
	</subDataset>
