	<subDataset name="bancos" uuid="312d8b75-79a6-420d-9c0a-a5fe2d339714">
		<parameter name="ur" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="id_periodo" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select c.nombre as tipo, f.nombre, b.descripcion as fuente, a.cuenta_contable, e.descripcion as estatus from entity.cuenta_bancaria a
full outer join catalog.catalogos b on b.id_cat = a.fuente_financiamiento
full outer join catalog.tipo_cuenta_bancaria c on c.id = a.id_tipo_cuenta
full outer join catalog.estatus_registro_catalogo e on e.id_estatus_cuenta = a.id_estatus
full outer join catalog.banco f on f.id=id_banco
where a.id_unidad_responsable = $P{ur} and a.id_periodo = $P{id_periodo} and a.id_estatus = 3]]>
		</queryString>
		<field name="tipo" class="java.lang.String"/>
		<field name="nombre" class="java.lang.String"/>
		<field name="fuente" class="java.lang.String"/>
		<field name="cuenta_contable" class="java.lang.String"/>
		<field name="estatus" class="java.lang.String"/>
	</subDataset>
	<parameter name="ur" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="id_periodo" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select a.id_unidad_responsable, usuario.nombre_usuario,
to_char(current_date, 'YYYY-MM-DD'), to_char(current_date,'yyyy') b from entity.cuenta_bancaria a
left join entity.usuario usuario on usuario.id_usuario = a.id_ultimo_usuario_modificador
where a.id_unidad_responsable =$P{ur} and id_periodo = $P{id_periodo}
limit 1]]>
	</queryString>
	<field name="id_unidad_responsable" class="java.lang.Long"/>
	<field name="nombre_usuario" class="java.lang.String"/>
	<field name="to_char" class="java.lang.String"/>
	<field name="b" class="java.lang.String"/>
