	<subDataset name="tabla" uuid="296b6409-41e0-4e78-a1b7-4dc56ad18158">
		<parameter name="ur" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="id_periodo" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT ccs.clave_cuenta AS clave_cuenta, ccs.descripcion AS nombre_cuenta, ccs.nivel AS nivel, emc.descripcion AS descripcion_nivel,
cna.nombre AS naturaleza_cuenta, cca.descripcion AS tipo_cuenta, ccca.descripcion AS clasificacion_cuenta
FROM catalog.cuentas_contables AS ccs
INNER JOIN catalog.naturaleza_cuenta AS cna ON (cna.id_naturaleza = ccs.naturaleza_cuenta)
INNER JOIN catalog.tipo_cuenta AS cca ON (cca.id_tipo_cuenta = ccs.tipo_cuenta)
INNER JOIN catalog.clasificacion_cuenta AS ccca ON (ccca.id_clasificacion = ccs.clasificacion_cuenta)
INNER JOIN entity.mascara_cuenta_contable AS emc ON (emc.id_unidad_responsable = ccs.ur AND emc.nivel = ccs.nivel AND emc.periodo = ccs.periodo)
WHERE ccs.ur = $P{ur} AND ccs.periodo = $P{id_periodo}]]>
		</queryString>
		<field name="clave_cuenta" class="java.lang.String"/>
		<field name="nombre_cuenta" class="java.lang.String"/>
		<field name="nivel" class="java.lang.Long"/>
		<field name="descripcion_nivel" class="java.lang.String"/>
		<field name="naturaleza_cuenta" class="java.lang.String"/>
		<field name="tipo_cuenta" class="java.lang.String"/>
		<field name="clasificacion_cuenta" class="java.lang.String"/>
	</subDataset>
	<parameter name="id_periodo" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ur" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT cpo.id_periodo AS Fecha, cs.descripcion AS Separacion, to_char(current_date,'dd/mm/yyyy'), u.nombre_usuario
From catalog.periodo AS cpo
INNER JOIN catalog.separador AS cs ON (cs.id_separador = cpo.id_separador)
INNER JOIN catalog.catalogos_presupuestales AS cps ON (cps.id_periodo = cpo.id_periodo)
INNER JOIN entity.usuario u ON (u.id_usuario = cpo.id_ultimo_usuario_modificador)
WHERE cpo.id_periodo = $P{id_periodo} and cpo.id_unidad_responsable = $P{ur}
LIMIT 1]]>
	</queryString>
	<field name="fecha" class="java.lang.Long"/>
	<field name="separacion" class="java.lang.String"/>
	<field name="to_char" class="java.lang.String"/>
	<field name="nombre_usuario" class="java.lang.String"/>
