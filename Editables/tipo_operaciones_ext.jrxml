	<subDataset name="query" uuid="d7af409e-b275-4bea-b1ea-6531108870b4">
		<parameter name="periodo" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select a.folio, a.tipo_operacion, c.descripcion As Genero, b.descripcion AS Afectacion, a.clave_cuenta_cont, a.id_tipo_operacion, a.periodo from catalog.tipos_operacion a
inner join catalog.tipo_afeccion b on b.id_tipo_afeccion = a.tipo_afeccion
inner join catalog.genero_contable c on c.id_genero_contable = a.genero_contable where periodo=$P{periodo}]]>
		</queryString>
		<field name="folio" class="java.lang.String"/>
		<field name="tipo_operacion" class="java.lang.String"/>
		<field name="genero" class="java.lang.String"/>
		<field name="afectacion" class="java.lang.String"/>
