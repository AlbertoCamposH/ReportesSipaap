	<subDataset name="tabla" uuid="87017644-adb2-45be-9773-7ee55aa7ed8a">
		<parameter name="ur" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="periodo" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="almacen" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT a.clave_alamcen, a.nombre, a.ubicacion_fisica, m.nombre AS municipio, c.descripcion AS centro_registro, a.responsable_alamcen
FROM operation.almacenes AS a
INNER JOIN catalog.municipio AS m ON (m.id_pais = a.id_pais AND m.id_estado = a.id_estado AND m.id_municipio = a.id_municipio)
INNER JOIN catalog.catalogos AS c ON (c.ur = a.ur AND c.id_periodo = a.id_periodo AND c.id_catalogo = a.id_centro_registro AND c.id_cat = a.centro_registro)
WHERE a.ur = $P{ur}  AND a.id_periodo = $P{periodo} AND a.id_almacenes = $P{almacen}]]>
		</queryString>
		<field name="clave_alamcen" class="java.lang.String"/>
		<field name="nombre" class="java.lang.String"/>
		<field name="ubicacion_fisica" class="java.lang.String"/>
		<field name="municipio" class="java.lang.String"/>
		<field name="centro_registro" class="java.lang.String"/>
		<field name="responsable_alamcen" class="java.lang.Long"/>
	</subDataset>
