		</parameter>
		<parameter name="id_devengado_ing" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT r.folio_asociado, r.importe, ($P{importe_total} - r.importe) AS devengado_no
FROM operation.recaudacion_ingresos_aport_min AS r
WHERE r.ur = $P{ur} AND r.id_periodo = $P{periodo} AND id_devengado_ing = $P{id_devengado_ing}]]>
		</queryString>
		<field name="folio_asociado" class="java.lang.String"/>
		<field name="importe" class="java.math.BigDecimal"/>
		<field name="devengado_no" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="suma" uuid="893be897-ab01-44df-a9e8-1fb0366278b8">
		<parameter name="ur" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="periodo" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="id_devengado_ing" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="importe_total" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT SUM(suma.devengado_no)
FROM
	(SELECT r.folio_asociado, r.importe, ($P{importe_total} - r.importe) AS devengado_no
	FROM operation.recaudacion_ingresos_aport_min AS r) AS suma,
operation.recaudacion_ingresos_aport_min AS r
WHERE r.ur = $P{ur} AND r.id_periodo = $P{periodo} AND id_devengado_ing = $P{id_devengado_ing}]]>
		</queryString>
		<field name="sum" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="ur" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="periodo" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="usuario" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="id_devengado_ing" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT d.folio, d.periodo, cr.descripcion AS ramo, cf.descripcion AS fondo_aportacion, d.importe_total
FROM operation.devengado_ingreso_aport_min AS d
INNER JOIN catalog.catalogo_ramos AS cr ON (cr.ur = d.ur AND cr.periodo = d.periodo AND cr.id_ramo = d.ramo)
INNER JOIN catalog.catalogo_fondo_aportacion AS cf ON (cf.id_catalogo = d.fondo_aportacion)
WHERE d.ur = $P{ur} AND d.periodo = $P{periodo} AND d.id_devengado_ing = $P{id_devengado_ing}
LIMIT 1]]>
	</queryString>
