	<subDataset name="tabla" uuid="f82455ed-57d8-417d-9233-89045d74903c">
		<parameter name="ur" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="id_periodo" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT pds.periodo, pds.id_cog AS consecutivo, pds.folio_partida,cs.descripcion AS programacion, c.descripcion
FROM catalog.partidas_duep_simultaneo AS pds
INNER JOIN catalog.catalogos AS c ON (c.ur = pds.ur AND c.id_periodo = pds.periodo AND c.id_catalogo = pds.id_cat_cog AND c.id_cat = pds.id_cog)
INNER JOIN catalog.catalogos AS cs ON (cs.ur = pds.ur AND cs.id_periodo = pds.periodo AND cs.id_catalogo = pds.id_cat_programa AND cs.id_cat = pds.id_programa)
WHERE pds.ur = $P{ur} AND pds.periodo = $P{id_periodo}]]>
		</queryString>
		<field name="periodo" class="java.lang.Long"/>
		<field name="consecutivo" class="java.lang.String"/>
		<field name="folio_partida" class="java.lang.Long"/>
		<field name="programacion" class="java.lang.String"/>
		<field name="descripcion" class="java.lang.String"/>
	</subDataset>
	<parameter name="ur" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="id_periodo" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT u.nombre_usuario FROM catalog.partidas_duep_simultaneo AS pds
INNER JOIN entity.usuario AS u ON (u.id_usuario = pds.ultimo_usuario_modificador)
WHERE pds.ur = $P{ur} AND pds.periodo = $P{id_periodo}
LIMIT 1]]>
	</queryString>
	<field name="nombre_usuario" class="java.lang.String"/>
