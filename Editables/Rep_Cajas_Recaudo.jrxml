		</parameter>
		<queryString>
			<![CDATA[SELECT cr.clave_caja_recaudacion,cr.descripcion,cr.serie, cr.ubicacion_fisica,
CONCAT(nombre,' ',primer_apellido, ' ', segundo_apellido) AS responsable, cr.fondo_asignado, cr.estatus
FROM operation.caja_recaudacion cr
INNER JOIN entity.usuario u ON u.id_usuario = responsable
WHERE cr.ur = $P{ur}
AND cr.periodo = $P{periodo}]]>
		</queryString>
