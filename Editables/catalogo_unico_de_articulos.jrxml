	<subDataset name="consulta" uuid="a9516bca-0bc3-4f78-b4b8-38ea45993c4c">
		<parameter name="ur" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="periodo" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT (art.nivel) AS "Clave de nivel", (ma.descripcion) AS "Descripción de nivel", (art.clave_articulo) AS "Clave", (art.descripcion_articulo) AS "Descripción", (um.descripcion) AS "Unidad de medida",CASE WHEN art.afectable = false THEN 'NO AFECTABLE' WHEN art.afectable = true THEN 'AFECTABLE' ELSE 'NA' END AS "Afectable", (art.clasificacion_objeto_gasto) AS "Clasificador por Objeto de Gasto", (art.descripcion_objeto_gasto) AS "Descripción COG", (ec.descripcion) AS "Estatus"
FROM catalog.articulos art
INNER JOIN catalog.estatus_cuenta ec ON ec.id_estatus_cuenta = CAST(art.estatus_cuenta AS bigint)
INNER JOIN entity.mascara_articulo ma ON ma.nivel = art.nivel
INNER JOIN catalog.unidad_medida um ON um.id_unidad_medida = art.unidad_medida
WHERE unidad_medida = um.id_unidad_medida
AND art.periodo =$P{periodo} AND um.id_periodo = art.periodo AND ma.id_periodo = art.periodo
AND art.ur = $P{ur} AND um.id_unidad_responsable = art.ur AND ma.id_unidad_responsable = art.ur
ORDER BY art.nivel]]>
		</queryString>
		<field name="Clave de nivel" class="java.lang.Long"/>
		<field name="Descripción de nivel" class="java.lang.String"/>
		<field name="Clave" class="java.lang.String"/>
		<field name="Descripción" class="java.lang.String"/>
		<field name="Unidad de medida" class="java.lang.String"/>
		<field name="Afectable" class="java.lang.String"/>
		<field name="Clasificador por Objeto de Gasto" class="java.lang.Long"/>
		<field name="Descripción COG" class="java.lang.String"/>
		<field name="Estatus" class="java.lang.String"/>
	</subDataset>
	<parameter name="ur" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="periodo" class="java.lang.Long">
		<defaultValueExpression><![CDATA[$P{periodo}]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select a.id_unidad_responsable, to_char(current_date,'dd/mm/yyyy'), to_char(current_date,'yyyy') b, u.nombre_usuario
FROM entity.usuario u
INNER JOIN entity.mascara_articulo a ON u.id_usuario = a.ultimo_usuario_modificador
where a.id_unidad_responsable =$P{ur}
LIMIT 1]]>
	</queryString>
	<field name="id_unidad_responsable" class="java.lang.Long"/>
	<field name="to_char" class="java.lang.String"/>
	<field name="b" class="java.lang.String"/>
	<field name="nombre_usuario" class="java.lang.String"/>
