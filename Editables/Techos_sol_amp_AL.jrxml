	<subDataset name="datos" uuid="72f8ff32-1055-4ee8-bc9a-dd1ac54475ad">
		<parameter name="ur" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="periodo" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="folio" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT DISTINCT folio_req, id_compra, descripcion_especifica, rd.clave_presupuestal,importe_total, importe_adjudicado, pendiente_asignar, pe.por_distrubuir
FROM operation.requisicion_detalle rd
FULL OUTER JOIN operation.presupuesto_egresos_det pe ON rd.clave_presupuestal = pe.clave_presupuestal
WHERE rd.ur = $P{ur} AND rd.periodo = $P{periodo}
AND rd.folio_req = $P{folio}]]>
		</queryString>
		<field name="folio_req" class="java.lang.Long"/>
		<field name="id_compra" class="java.lang.String"/>
		<field name="descripcion_especifica" class="java.lang.String"/>
		<field name="clave_presupuestal" class="java.lang.String"/>
		<field name="importe_total" class="java.math.BigDecimal"/>
		<field name="importe_adjudicado" class="java.math.BigDecimal"/>
		<field name="pendiente_asignar" class="java.math.BigDecimal"/>
		<field name="por_distrubuir" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="ur" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="periodo" class="java.lang.Long"/>
	<parameter name="folio" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select r.*, a.id_unidad_responsable, to_char(current_date,'dd/mm/yyyy'), to_char(current_date,'yyyy') b, u.nombre_usuario
FROM
(
SELECT proceso_adjudicacion, fecha_solicitud_adj
FROM operation.requisicion
WHERE ur = $P{ur} AND periodo = $P{periodo} AND folio_req = $P{folio}
)AS r, entity.usuario u
INNER JOIN entity.mascara_articulo a ON u.id_usuario = a.ultimo_usuario_modificador
where a.id_unidad_responsable =$P{ur}
LIMIT 1]]>
	</queryString>
	<field name="proceso_adjudicacion" class="java.lang.String"/>
	<field name="fecha_solicitud_adj" class="java.sql.Timestamp"/>
	<field name="id_unidad_responsable" class="java.lang.Long"/>
	<field name="to_char" class="java.lang.String"/>
	<field name="b" class="java.lang.String"/>
	<field name="nombre_usuario" class="java.lang.String"/>
