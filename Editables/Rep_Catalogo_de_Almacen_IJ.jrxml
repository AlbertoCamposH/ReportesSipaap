	<subDataset name="tabla" uuid="ea0edb8d-310a-43ee-bd6d-229196605c49">
		<parameter name="id_almacenes" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="periodo" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="ur" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select almacen.clave_alamcen, almacen.nombre, almacen.ubicacion_fisica, municipio.nombre as municipio, almacen.centro_registro, CONCAT( responsable.nombre,' ', responsable.primer_apellido, ' ', responsable.segundo_apellido) AS responsable from operation.almacenes almacen
inner join catalog.municipio municipio on municipio.id_pais = almacen.id_pais and municipio.id_estado = almacen.id_estado and municipio.id_municipio = almacen.id_municipio
inner join entity.usuario responsable on responsable.id_unidad_responsable = almacen.ur and responsable.id_usuario = almacen.responsable_alamcen
where id_almacenes = $P{id_almacenes} and ur = $P{ur} and periodo = $P{periodo}]]>
		</queryString>
		<field name="clave_alamcen" class="java.lang.String"/>
		<field name="nombre" class="java.lang.String"/>
		<field name="ubicacion_fisica" class="java.lang.String"/>
		<field name="municipio" class="java.lang.String"/>
		<field name="centro_registro" class="java.lang.String"/>
		<field name="responsable" class="java.lang.String"/>
	</subDataset>
