	<queryString>
		<![CDATA[select datos.*,  a.id_unidad_responsable, to_char(current_date,'dd/mm/yyyy'), to_char(current_date,'yyyy') b, u.nombre_usuario
FROM
(
SELECT fuente_financiamiento, porcentaje, '' adjuntar_archivo
FROM operation.porcentaje_ampliacion
WHERE periodo = $P{periodo}
AND ur = $P{ur}
) AS datos, entity.usuario u
INNER JOIN entity.mascara_articulo a ON u.id_usuario = a.ultimo_usuario_modificador
where a.id_unidad_responsable =$P{ur}
LIMIT 1]]>
	</queryString>
