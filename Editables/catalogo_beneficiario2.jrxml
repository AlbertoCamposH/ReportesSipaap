	<subDataset name="tabla" uuid="4160d508-fab5-4b84-b03b-16defba8a700">
		<parameter name="ur" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="id_periodo" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select beneficiario.fecha_registro, beneficiario.id_beneficiario, tipo_beneficiario.tipo, beneficiario.razon_social,
concat_ws(' ', beneficiario.nombre, beneficiario.primer_apellido) nombre,
beneficiario.rfc, beneficiario.curp,
concat_ws(' ', beneficiario_domicilio.colonia, beneficiario_domicilio.calle, beneficiario_domicilio.numero) domicilio,
beneficiario_cuenta_bancaria.cuenta_bancaria, estatus.estatus
from entity.beneficiario beneficiario
full outer join entity.beneficiario_domicilio beneficiario_domicilio on beneficiario_domicilio.id_beneficiario = beneficiario.id_beneficiario
full outer join entity.beneficiario_cuenta_bancaria beneficiario_cuenta_bancaria on beneficiario_cuenta_bancaria.id_beneficiario = beneficiario.id_beneficiario
full outer join entity.tipo_beneficiario tipo_beneficiario on tipo_beneficiario.id_tipo_beneficiario = beneficiario.id_tipo_beneficiario
full outer join catalog.estatus estatus on estatus.id = beneficiario.id_estatus_beneficiario
where beneficiario.id_unidad_responsable = $P{ur} and beneficiario.id_periodo=$P{id_periodo}]]>
		</queryString>
		<field name="fecha_registro" class="java.sql.Timestamp"/>
		<field name="id_beneficiario" class="java.lang.Long"/>
		<field name="tipo" class="java.lang.String"/>
		<field name="razon_social" class="java.lang.String"/>
		<field name="nombre" class="java.lang.String"/>
		<field name="rfc" class="java.lang.String"/>
		<field name="curp" class="java.lang.String"/>
		<field name="domicilio" class="java.lang.String"/>
		<field name="cuenta_bancaria" class="java.lang.String"/>
		<field name="estatus" class="java.lang.String"/>
	</subDataset>
	<parameter name="ur" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="id_periodo" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="id_beneficiario" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select a.id_unidad_responsable, usuario.nombre_usuario, to_char(current_date,'dd/mm/yyyy'), to_char(current_date,'yyyy') b from entity.beneficiario a
left join entity.usuario usuario on usuario.id_usuario = a.id_ultimo_usuario_modificador
where a.id_unidad_responsable = $P{ur}and a.id_periodo = $P{id_periodo} and a.id_beneficiario= $P{id_beneficiario} limit 1]]>
	</queryString>
