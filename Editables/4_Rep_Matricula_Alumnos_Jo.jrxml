	<subDataset name="Matriculas" uuid="83522e09-26c2-40dc-b48f-ba7087318f94">
		<parameter name="ur" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="No" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT m.clave_beneficiario, (b.nombre||' '|| b.primer_apellido||' '||b.segundo_apellido) AS nombre, b.curp,
tb.tipo, m.folio_matricula, c.descripcion AS carrera, m.fecha_ingreso, em.descripcion
FROM entity.matriculas m
INNER JOIN entity.beneficiario b ON b.id_beneficiario = m.clave_beneficiario
INNER JOIN entity.tipo_beneficiario tb ON b.id_tipo_beneficiario = tb.id_tipo_beneficiario
INNER JOIN catalog.carrera c ON m.carrera = c.id_carrera
INNER JOIN catalog.estatus_matricula em ON em.id_estatus_mat = m.estatus
WHERE m.ur = $P{ur} AND m.estatus = 1;]]>
		</queryString>
		<field name="clave_beneficiario" class="java.lang.Long"/>
		<field name="nombre" class="java.lang.String"/>
		<field name="curp" class="java.lang.String"/>
		<field name="tipo" class="java.lang.String"/>
		<field name="folio_matricula" class="java.lang.Long"/>
		<field name="carrera" class="java.lang.String"/>
		<field name="fecha_ingreso" class="java.sql.Timestamp"/>
		<field name="descripcion" class="java.lang.String"/>
	</subDataset>
	<parameter name="ur" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="usuario" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT m.ur FROM entity.matriculas m
INNER JOIN entity.beneficiario b ON m.clave_beneficiario = b.id_beneficiario WHERE m.ur = $P{ur} LIMIT 1;]]>
	</queryString>
	<field name="ur" class="java.lang.Long"/>
