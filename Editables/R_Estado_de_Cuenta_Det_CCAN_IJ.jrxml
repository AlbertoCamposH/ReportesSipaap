		</parameter>
		<queryString>
			<![CDATA[select id_detalle_compromiso as bloque, id_compromiso_imp as duep_asociado, fecha_elaboracion, concepto, importe_devengado,  saldo_compromiso  from operation.compromisos_anual_det where ur = $P{ur} and id_periodo = $P{periodo} and id_compromiso = $P{id_compromiso}]]>
		</queryString>
		<field name="bloque" class="java.lang.Long"/>
		<field name="duep_asociado" class="java.lang.Long"/>
