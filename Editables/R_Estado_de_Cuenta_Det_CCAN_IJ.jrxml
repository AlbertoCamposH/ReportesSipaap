	<queryString>
		<![CDATA[select compromiso.id_beneficiario, CONCAT( beneficiario.nombre,' ', beneficiario.primer_apellido, ' ', beneficiario.segundo_apellido) AS nombre, compromiso.centro_registro, catalogo.descripcion, compromiso.concepto, compromiso.id_duep, compromiso.fecha_elaboracion from operation.compromisos_anual compromiso
inner join entity.beneficiario beneficiario on beneficiario.id_beneficiario = compromiso.id_beneficiario
inner join catalog.catalogos catalogo on catalogo.ur=compromiso.ur and catalogo.id_periodo=compromiso.id_periodo and compromiso.id_centro_registro = catalogo.id_catalogo and catalogo.id_cat=compromiso.centro_registro
where compromiso.ur = $P{ur} and compromiso.id_periodo = $P{periodo} and compromiso.id_compromiso = $P{id_compromiso}
]]>
	</queryString>
