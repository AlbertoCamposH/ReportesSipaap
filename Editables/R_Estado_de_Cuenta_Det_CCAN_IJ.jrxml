		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select compromiso.id_beneficiario, CONCAT( beneficiario.nombre,' ', beneficiario.primer_apellido, ' ', beneficiario.segundo_apellido) AS nombre, compromiso.centro_registro, catalogo.descripcion, compromiso.concepto, compromiso.id_duep, compromiso.fecha_elaboracion, total_comprometido, total_devengado from operation.compromisos_anual compromiso
full outer join entity.beneficiario beneficiario on beneficiario.id_beneficiario = compromiso.id_beneficiario
full outer join catalog.catalogos catalogo on catalogo.ur=compromiso.ur and catalogo.id_periodo=compromiso.id_periodo and compromiso.id_centro_registro = catalogo.id_catalogo and catalogo.id_cat=compromiso.centro_registro
where compromiso.ur = $P{ur} and compromiso.id_periodo = $P{periodo} and compromiso.id_compromiso = $P{id_compromiso} limit 1]]>
	</queryString>
