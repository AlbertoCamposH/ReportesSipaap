	<subDataset name="dataset1" uuid="a53a23b5-0285-4004-80d6-16f7a3aecce2">
		<parameter name="id_periodo" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="id_catalogo" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT catalogos.clave_cuenta AS clave_nivel, sma.descripcion AS descripcion_nivel, clasificador.clave As clave_registro, clasificador.descripcion AS descripcion_registro,
categoria.descripcion AS clasificacion_programatica ,catalogos.afectable AS afectable, catalogos.estatus AS estatus
FROM catalog.catalogos AS catalogos
INNER JOIN catalog.clasificador_economico AS clasificador ON (clasificador.id_periodo = catalogos.id_periodo)
INNER JOIN catalog.programa_presupuestario AS programa ON (catalogos.id_cat = programa.id_cat)
INNER JOIN catalog.categoria_programatica AS categoria ON (programa.categoria_programatica = categoria.id_categoria_programatica)
INNER JOIN catalog.segmento_mascara AS sma ON (sma.id_periodo = catalogos.id_periodo)
WHERE catalogos.id_catalogo = $P{id_catalogo} AND catalogos.id_periodo =  $P{id_periodo}
AND catalogos.id_catalogo = sma.id_catalogo AND catalogos.nivel = sma.nivel]]>
		</queryString>
		<field name="clave_nivel" class="java.lang.String"/>
		<field name="descripcion_nivel" class="java.lang.String"/>
		<field name="clave_registro" class="java.lang.String"/>
		<field name="descripcion_registro" class="java.lang.String"/>
		<field name="clasificacion_programatica" class="java.lang.String"/>
		<field name="afectable" class="java.lang.Boolean"/>
		<field name="estatus" class="java.lang.Long"/>
	</subDataset>
	<parameter name="id_catalogo" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="id_periodo" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT cps.nombre AS nombre,  to_char(current_date,'dd/mm/yyyy'), u.nombre_usuario FROM catalog.catalogos_presupuestales AS cps
INNER JOIN entity.usuario u ON(u.id_usuario = cps.id_usuario_modificador)
WHERE id_catalogo = $P{id_catalogo} AND id_periodo = $P{id_periodo}]]>
	</queryString>
	<field name="nombre" class="java.lang.String"/>
	<field name="to_char" class="java.lang.String"/>
