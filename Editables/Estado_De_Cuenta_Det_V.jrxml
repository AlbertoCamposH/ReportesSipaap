		</parameter>
		<queryString>
			<![CDATA[SELECT rbd.id_af_receprcion_bienes, articulo, folio_recepcion, fecha_recepcion, unidad_medida,
cantidad_solicitada, cantidad_recibida, por_recibir, presupuesto_comprometido,
presupuesto_devengado, comprometido_no_devendago
FROM operation.af_receprcion_bienes_det rbd
WHERE rbd.ur = $P{ur} AND rbd.periodo = $P{periodo} AND id_af_receprcion_bienes_det = $P{folio}]]>
		</queryString>
		<field name="id_af_receprcion_bienes" class="java.lang.String"/>
		<field name="articulo" class="java.lang.String"/>
		<field name="folio_recepcion" class="java.lang.String"/>
		<field name="fecha_recepcion" class="java.sql.Date"/>
		<field name="unidad_medida" class="java.lang.Long"/>
		<field name="cantidad_solicitada" class="java.math.BigDecimal"/>
		<field name="cantidad_recibida" class="java.math.BigDecimal"/>
		<field name="por_recibir" class="java.math.BigDecimal"/>
		<field name="presupuesto_comprometido" class="java.math.BigDecimal"/>
		<field name="presupuesto_devengado" class="java.math.BigDecimal"/>
		<field name="comprometido_no_devendago" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="totales" uuid="00e46b47-37cb-4402-ad68-9cc646897374">
		<parameter name="ur" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="periodo" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="folio" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT SUM(cantidad_solicitada)AS cantidad_solicitada_total, SUM(cantidad_recibida)AS cantidad_recibida_total
, SUM (por_recibir) AS por_recibir_total, SUM(presupuesto_comprometido)AS presupuesto_comprometido_total
, SUM(presupuesto_devengado)AS presupuesto_devengado, SUM(comprometido_no_devendago)AS presupuesto_no_devengado
FROM operation.af_receprcion_bienes_det rbd
WHERE rbd.ur = $P{ur} AND rbd.periodo = $P{periodo} AND id_af_receprcion_bienes_det = $P{folio}]]>
		</queryString>
		<field name="cantidad_solicitada_total" class="java.math.BigDecimal"/>
		<field name="cantidad_recibida_total" class="java.math.BigDecimal"/>
		<field name="por_recibir_total" class="java.math.BigDecimal"/>
		<field name="presupuesto_comprometido_total" class="java.math.BigDecimal"/>
		<field name="presupuesto_devengado" class="java.math.BigDecimal"/>
		<field name="presupuesto_no_devengado" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="ur" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="periodo" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="folio" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select datos.*,a.id_unidad_responsable, to_char(current_date,'dd/mm/yyyy'), to_char(current_date,'yyyy') b, u.nombre_usuario
FROM
(
SELECT DISTINCT arb.beneficiario, arb.centro_registro,af.concepto, arb.duep, arb.contrato_orden_compra, arb.contrato, arb.fecha_registro
FROM operation.af_receprcion_bienes arb
INNER JOIN operation.activo_fijo af ON af.ur =  arb.ur
INNER JOIN operation.af_receprcion_bienes_det arbd ON arbd.ur =  arb.ur
WHERE arb.ur = $P{ur} AND arb.periodo = $P{periodo}  AND arbd.id_af_receprcion_bienes_det = $P{folio}
AND arb.periodo = arbd.periodo
) AS datos
,entity.usuario u
INNER JOIN entity.mascara_articulo a ON u.id_usuario = a.ultimo_usuario_modificador
where a.id_unidad_responsable =$P{ur}
LIMIT 1]]>
	</queryString>
	<field name="beneficiario" class="java.lang.Long"/>
	<field name="centro_registro" class="java.lang.Long"/>
	<field name="concepto" class="java.lang.String"/>
	<field name="duep" class="java.lang.String"/>
	<field name="contrato_orden_compra" class="java.lang.String"/>
	<field name="contrato" class="java.lang.String"/>
	<field name="fecha_registro" class="java.sql.Timestamp"/>
	<field name="id_unidad_responsable" class="java.lang.Long"/>
	<field name="to_char" class="java.lang.String"/>
	<field name="b" class="java.lang.String"/>
	<field name="nombre_usuario" class="java.lang.String"/>
