	<subDataset name="dataset" uuid="83505dfa-a98d-4c33-9e6d-a13a6b98fbf4">
		<parameter name="id_unidad_responsable" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select u.id_usuario, CONCAT( u.nombre,' ', u.primer_apellido, ' ', u.segundo_apellido) AS nombre, u.curp, d.nombre as direccion, a.nombre as area, p.nombre as puesto, cp.nombre as puesto_superior,
case when u.estatus = True then 'Activo' when u.estatus = False then 'in Activo' else 'Na' end as estatus
from catalog.unidad_responsable ur
inner join entity.usuario u ON (u.id_unidad_responsable = ur.id_unidad_responsable)
inner join catalog.direccion d ON (d.id_direccion = u.id_direccion)
inner join catalog.area a on (a.id_direccion = d.id_direccion)
inner join catalog.puesto p on (p.id_puesto = u.puesto)
inner join catalog.puesto cp on (cp.id_puesto = u.id_puesto_superior)
where ur.id_unidad_responsable = $P{id_unidad_responsable}
order by id_usuario]]>
		</queryString>
		<field name="id_usuario" class="java.lang.Long"/>
		<field name="nombre" class="java.lang.String"/>
		<field name="curp" class="java.lang.String"/>
		<field name="direccion" class="java.lang.String"/>
		<field name="area" class="java.lang.String"/>
		<field name="puesto" class="java.lang.String"/>
		<field name="puesto_superior" class="java.lang.String"/>
		<field name="estatus" class="java.lang.String"/>
	</subDataset>
	<parameter name="id_unidad_responsable" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select a.id_unidad_responsable, to_char(current_date,'dd/mm/yyyy'), to_char(current_date,'yyyy') b, u.nombre_usuario from catalog.unidad_responsable a
INNER JOIN entity.usuario u ON (a.id_ultimo_usuario_modificador = u.id_usuario)
where a.id_unidad_responsable =$P{id_unidad_responsable}
limit 1]]>
	</queryString>
	<field name="id_unidad_responsable" class="java.lang.Long"/>
	<field name="to_char" class="java.lang.String"/>
	<field name="b" class="java.lang.String"/>
	<field name="nombre_usuario" class="java.lang.String"/>
