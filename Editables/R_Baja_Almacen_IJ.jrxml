			<![CDATA[select activo.clave_articulo, articulo.descripcion_articulo, activo.fecha_factura as fecha_salida, activo.marca, activo.modelo, activo.serie, activo.color, activo.unidad_medida, activo.cantidad, activo.costo_unitario from operation.activo_fijo_det_aux activo
