	<subDataset name="tabla" uuid="f032e604-6803-4f58-8548-8689437f55c9">
		<queryString language="SQL">
			<![CDATA[select costeo.id_periodo, costeo.tipo_costeo, costeo.fecha_elaboracion, CONCAT( usuario.nombre,' ', usuario.primer_apellido, ' ', usuario.segundo_apellido) AS usuario_alta from catalog.tipo_costeo costeo
inner join entity.usuario usuario on usuario.id_unidad_responsable = costeo.ur and usuario.id_usuario = costeo.usuario_alta
where id_tipo_costeo = 1 and ur = 1 and id_periodo = 2019]]>
		</queryString>
		<field name="id_periodo" class="java.lang.Long"/>
		<field name="tipo_costeo" class="java.lang.String"/>
