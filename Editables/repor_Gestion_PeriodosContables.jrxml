	<queryString>
		<![CDATA[SELECT r.nombre,to_char(current_date,'dd/mm/yyyy'), to_char(current_date,'yyyy'), p.id_periodo AS periodo, e.nombre AS estatus, s.caracter
FROM catalog.unidad_responsable r
INNER JOIN catalog.periodo p ON (p.id_unidad_responsable = r.id_unidad_responsable)
INNER JOIN catalog.periodo_estatus e ON (e.id_periodo_estatus = p.id_estatus)
INNER JOIN catalog.separador s ON (s.id_separador = p.id_separador)
WHERE p.id_unidad_responsable = $P{id_responsable} and p.id_periodo = $P{id_periodo}]]>
	</queryString>
